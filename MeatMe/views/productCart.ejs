<%- include("partials/header")  %>
<section class="cart-section">
    <div class="card" class="container row col-md-12">
        <div class="cart-table justify-content-center mx-auto">
            <h3>Tus Compras</h3>
            <div class="cart-table-warp">
                <table>
                    <thead>
                        <tr>
                            <th class="producto-th">Producto</th>
                            <th class="cantidad-th">Cantidad</th>
                            <th class="precio-th">Precio</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="product-col">
                                <img class="thumb" src="public/img/costeletas.jpeg" alt="">
